<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mainpage">

    <meta-data
        android:name="android.max_aspect"
        android:value="2.1" />

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new2"
        android:label="ReviewHouse"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_new2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".user.ReviewPage" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".user.AfterSearch" android:parentActivityName=".MainActivity" />
        <activity android:name=".HouseUpdate" android:parentActivityName=".MainActivity" />
        <activity android:name=".user.ReviewUpdate" android:parentActivityName=".MainActivity"/>
        <activity android:name=".HouseRegister" android:parentActivityName=".MainActivity" />
        <activity android:name=".user.ReviewRegister" android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailHousePage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginPage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".JoinPage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".OwnerMypage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".user.UserMypage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".user.SearchPage"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>